# API Endpoint Discovery Configuration

# Thời gian delay trước khi chạy discovery sau khi app start (milliseconds)
# Mặc định: 2000ms (2 giây)
DISCOVERY_DELAY_MS=2000

# Bật/tắt discovery process
# Mặc định: true
# Set false để disable discovery hoàn toàn (cho testing hoặc môi trường đặc biệt)
DISCOVERY_ENABLED=true
